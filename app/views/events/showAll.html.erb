 <% @events.each do |event| %>
    <h1><%= event.title %></h1>
    <p><%= event.description %></p>
    <p>Date de début : <%= event.start_date %></p>
    <p>Durée : <%= event.duration %> heures</p>
    <p>Prix : <%= event.price %> €</p>
    <p>Lieu : <%= event.location %></p>
    <p>Créé par : <%= event.user.email %></p>
<% end %>
